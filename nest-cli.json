{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/customer-portal/src",
  "entryFile": "main",
  "monorepo": true,
  "root": "apps/customer-portal",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/customer-portal/tsconfig.app.json"
  },
  "projects": {
    "customer-portal": {
      "type": "application",
      "root": "apps/customer-portal",
      "entryFile": "main",
      "sourceRoot": "apps/customer-portal/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customer-portal/tsconfig.app.json"
      }
    },
    "library-api": {
      "type": "library",
      "root": "../cakrawala-hub-library-api",
      "sourceRoot": "../cakrawala-hub-library-api/src",
      "entryFile": "index",
      "compilerOptions": {
        "tsConfigPath": "../cakrawala-hub-library-api/tsconfig.json"
      }
    },
    "partner-portal": {
      "type": "application",
      "root": "apps/partner-portal",
      "entryFile": "main",
      "sourceRoot": "apps/partner-portal/src",
      "compilerOptions": {
        "tsConfigPath": "apps/partner-portal/tsconfig.app.json"
      }
    }
  }
}